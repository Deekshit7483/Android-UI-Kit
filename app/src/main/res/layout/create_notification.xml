<androidx.coordinatorlayout.widget.CoordinatorLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/_coordinator"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<com.google.android.material.appbar.AppBarLayout
		android:id="@+id/_app_bar"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:theme="@style/AppTheme.AppBarOverlay">
		<androidx.appcompat.widget.Toolbar
			android:id="@+id/_toolbar"
			android:layout_width="match_parent"
			android:layout_height="?attr/actionBarSize"
			android:background="?attr/colorPrimary"
			app:popupTheme="@style/AppTheme.PopupOverlay" />
	</com.google.android.material.appbar.AppBarLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		app:layout_behavior="@string/appbar_scrolling_view_behavior">
		<LinearLayout
			android:id="@+id/linear79"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/linear58"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear59"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/linear60"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:gravity="center_vertical"
						android:orientation="horizontal">
						<ImageView
							android:id="@+id/imageview1"
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:padding="3dp"
							android:src="@drawable/logo"
							android:scaleType="fitXY" />
						<TextView
							android:id="@+id/textview21"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="3dp"
							android:text="Android Ui kit"
							android:textSize="16sp"
							android:textStyle="bold"
							android:textColor="#2196F3" />
						<TextView
							android:id="@+id/timeee"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:text="12:00"
							android:textSize="16sp"
							android:textStyle="bold"
							android:textColor="#616161" />
					</LinearLayout>
					<TextView
						android:id="@+id/title"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="5dp"
						android:paddingTop="5dp"
						android:paddingRight="5dp"
						android:text="App name"
						android:textSize="18sp"
						android:textColor="#000000"
						android:singleLine="true" />
					<TextView
						android:id="@+id/text"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="5dp"
						android:paddingRight="5dp"
						android:paddingBottom="5dp"
						android:text="Hello everyone"
						android:textSize="14sp"
						android:textColor="#000000"
						android:singleLine="true" />
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
		<ScrollView
			android:id="@+id/vscroll1"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:overScrollMode="never">
			<LinearLayout
				android:id="@+id/linear1"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:orientation="vertical">
				<com.google.android.gms.ads.AdView
					android:id="@+id/adview1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="15dp"
					android:layout_marginBottom="15dp"
					app:adSize="SMART_BANNER"
					app:adUnitId="ca-app-pub-5995827559592413/9006206954" />
				<LinearLayout
					android:id="@+id/l"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingBottom="8dp"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/linear65"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear76"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="vertical">
							<LinearLayout
								android:id="@+id/linear75"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal">
								<TextView
									android:id="@+id/i"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="Activity"
									android:textSize="20sp"
									android:textStyle="bold"
									android:textColor="#2196F3"
									android:alpha="0.6" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear77"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="vertical">
								<TextView
									android:id="@+id/textview33"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="Activity"
									android:textSize="18sp"
									android:textColor="#000000" />
								<TextView
									android:id="@+id/textview34"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="In which activity you will use the generated code\?"
									android:textSize="16sp"
									android:textColor="#000000" />
								<EditText
									android:id="@+id/activity"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="MainActivity"
									android:textSize="16sp"
									android:textColor="#000000"
									android:hint="Activity name (e.g. MainActivity)"
									android:textColorHint="#607D8B"
									android:singleLine="true"
									android:imeOptions="actionDone" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear84"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal">
								<TextView
									android:id="@+id/p2"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="Information"
									android:textSize="20sp"
									android:textStyle="bold"
									android:textColor="#2196F3"
									android:alpha="0.6"
									android:layout_weight="1" />
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear62"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:orientation="vertical">
						<TextView
							android:id="@+id/textview25"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="Title"
							android:textSize="18sp"
							android:textColor="#000000" />
						<EditText
							android:id="@+id/titlee"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="App name"
							android:textSize="16sp"
							android:textColor="#000000"
							android:hint="Title"
							android:textColorHint="#607D8B"
							android:singleLine="true"
							android:imeOptions="actionDone" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear63"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:orientation="vertical">
						<TextView
							android:id="@+id/textview26"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="Text"
							android:textSize="18sp"
							android:textColor="#000000" />
						<EditText
							android:id="@+id/textt"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="Hello everyone"
							android:textSize="16sp"
							android:textColor="#000000"
							android:hint="Text"
							android:textColorHint="#607D8B"
							android:singleLine="true"
							android:imeOptions="actionDone" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear66"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:orientation="horizontal">
						<Switch
							android:id="@+id/on"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="left"
							android:text="Ongoing notification"
							android:textSize="18sp"
							android:textColor="#000000" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear67"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:orientation="horizontal">
						<Switch
							android:id="@+id/ev"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="left"
							android:text="Enable vibration"
							android:textSize="18sp"
							android:textColor="#000000" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear68"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:orientation="vertical">
						<Switch
							android:id="@+id/bi"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="left|center_vertical"
							android:text="Show a badge to the app icon"
							android:textSize="18sp"
							android:textColor="#000000" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear83"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:orientation="horizontal">
						<Switch
							android:id="@+id/switch1"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="left"
							android:text="Show notification time"
							android:textSize="18sp"
							android:textColor="#000000" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear78"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:orientation="vertical">
						<TextView
							android:id="@+id/textview35"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="Icon"
							android:textSize="18sp"
							android:textColor="#000000" />
						<EditText
							android:id="@+id/edittext1"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="default_image"
							android:textSize="16sp"
							android:textColor="#000000"
							android:hint="Icon name"
							android:textColorHint="#607D8B"
							android:singleLine="true"
							android:imeOptions="actionDone" />
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/source_code"
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:padding="8dp"
					android:background="#000000"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/code"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Source code..."
						android:textSize="18sp"
						android:textColor="#FFFFFF"
						android:layout_weight="1" />
				</LinearLayout>
			</LinearLayout>
		</ScrollView>
	</LinearLayout>
	<com.google.android.material.floatingactionbutton.FloatingActionButton
		android:id="@+id/_fab"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_margin="16dp"
		android:layout_gravity="right|bottom"
		app:srcCompat="@drawable/code_2" />
</androidx.coordinatorlayout.widget.CoordinatorLayout>
